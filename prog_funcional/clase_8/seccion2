data N = Z | S N

-- Intenta describir representaciones unarias de nÃºmeros naturales

a)
    evalN :: N -> Int
    evalN Z = 0
    evalN (S n) = 1 + evalN N

    addN :: N -> N -> N
    addN Z n = n
    addN n Z = n
    addN (S n) (S n2) = (S (addN n n2))

    prodN :: N -> N -> N
    prodN Z n = Z
    prodN n Z = Z
    prodN (S n) (S n2) = addN (S n) (prodN (S n) n2)

    int2N :: Int -> n
    int2N 0 = Z
    int2N n = (S (int2N n - 1))

    
